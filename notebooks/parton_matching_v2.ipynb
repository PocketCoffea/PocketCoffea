   "source": [
    "events[\"MuonGood\"]     = lepton_selection(events, \"Muon\", \"dilepton\")\n",
    "events[\"ElectronGood\"] = lepton_selection(events, \"Electron\", \"dilepton\")\n",
    "leptons = ak.with_name( ak.concatenate( (events.MuonGood, events.ElectronGood), axis=1 ), name='PtEtaPhiMCandidate' )\n",
    "events[\"LeptonGood\"]   = leptons[ak.argsort(leptons.pt, ascending=False)]\n",
    "events[\"JetGood\"], _  = jet_selection(events, \"Jet\", \"dilepton\")"
   ]
  },
  {
   "cell_type": "code",
