    "@numba.njit\n",
    "def delta_pt_numba(jets, genJets, mask, builder):\n",
    "    for js , genJs, ms in zip(jets, genJets, mask):\n",
    "        builder.begin_list()\n",
    "        for j, g, m in zip(js, genJs, ms):\n",
    "            if m:\n",
    "                builder.append(abs(j-g))\n",
    "            else:\n",
    "                builder.append(None)\n",
    "        builder.end_list()\n",
    "    return builder"
